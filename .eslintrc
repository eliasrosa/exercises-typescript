{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "plugin:jest/style",
    "plugin:jest/recommended"
  ],
  "plugins": ["@typescript-eslint", "jest"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "no-console": "off",
    "arrow-body-style": "off",
    "import/prefer-default-export": "off"
  }
}
